<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        android:orientation="horizontal"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:paddingLeft="?attr/horizontalSpacingMargin"
        android:paddingRight="?attr/horizontalSpacingMargin"
        android:paddingTop="?attr/verticalSpacingSmall"
        android:paddingBottom="?attr/verticalSpacingSmall"
        >

    <LinearLayout
            android:id="@+id/atl_settings_item_icon_container"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:gravity="center"
            android:layout_marginRight="?attr/horizontalSpacingMedium"
            tools:ignore="RtlHardcoded"
            >

        <ImageView
                android:id="@+id/atl_settings_item_icon"
                android:layout_width="?attr/iconListSize"
                android:layout_height="?attr/iconListSize"
                android:layout_gravity="center"
                />

    </LinearLayout>

